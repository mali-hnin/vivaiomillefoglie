<% content_for(:title_tag) do %>
  Piante per Insetti e Uccelli, acquista online | Vivaio Millefoglie
<% end %>
<% content_for :meta_title, "Vendita online piante per insetti e uccelli | Vivaio Millefoglie" %>
<% content_for :meta_description, "Acquista online piante che attirano farfalle, insetti e uccelli. Le nostre piante sono utili a una gestione del giardino secondo natura e crescono in campo aperto con prodotti per l'agricoltura biologica. Scopri il resto del nostro catalogo." %>

<div class="bg">
  <%= render 'shared/navbar' %>
  <div class="white-bg">
    <%= render 'shared/catalogo_flash' %>
    <div class="catalogo-content">
      <nav aria-label="breadcrumb" class="breadcrumbs">
        <ul>
          <li><%= link_to "Catalogo", catalogo_path(anchor: 'catalogo'), class: "orange-text" %></li>
          <li class="bc-active" aria-current="page">Ambiente - insetti e uccelli</li>
        </ul>
      </nav>
      <h1 class="catalogo-mobile">Piante utili per insetti e uccelli</h1>
      <%= render 'filters_form', chapter: insetti_e_uccelli_plants_path %>
      <div class="filter-results">
        <%= render 'catalogo_clienti' %>
      </div>
    </div>
  </div>
</div>
